{
	"manifest_version": 3,
	"name": "GNews",
	"description": "Interactive News Extension",
	"version": "1.0.0",
	"icons": {
		"128": "icon_128.png"
	},
	"action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["storage", "scripting", "activeTab"]
}
